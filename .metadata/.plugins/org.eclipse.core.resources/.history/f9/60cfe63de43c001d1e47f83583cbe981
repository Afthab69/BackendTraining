package com.orthofx;

import java.util.*;

public class RepeatingSequence {

	static void substringEval(String str) {

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str, sub = "", x="",exp="";
		int k=1,start=0,end = 0,count=0,m=1,st=0,e=0;

		Scanner s = new Scanner(System.in);
		System.out.println("Enter String: ");
		str = s.nextLine();
		for (int i = 0; i < str.length(); ++i) {
//			if (Character.isDigit(str.charAt(i))) {
//				k = Integer.valueOf(Integer.parseInt(""+str.charAt(i)));
//			}
//			System.out.println(k);
			if (str.charAt(i) == '[' && count==0) {
				start = i;
				count = count+1;
			}
			if(str.charAt(i)==']') {
				end = i;
			}
		}
		k = Integer.valueOf(str.substring(0,start));
		sub = str.substring(start+1, end);
		for(int i=0;i<sub.length();++i) {
			if(Character.isAlphabetic(sub.charAt(i))) {
				exp = exp+i;
			}
			if(Character.isDigit(i)) {
				m = Integer.parseInt(""+sub.charAt(i));
				i=i+1;
				if(Character.isDigit(i)) {
					m =m*10 + Integer.parseInt(""+sub.charAt(i));
				}
				else if(sub.charAt(i) == '[') {
					st = i+1;
					while(sub.charAt(i)!=']') {
						i=i+1;
						e=i;
					}
				}
			}
			
		}
		for(int z = 0;z<m;++z) {
			exp = exp+sub.substring(st, e);
		}
		for(int i=0;i<k;++i) {
			x += sub;
		}
		System.out.println(x+" "+exp);
	}
}

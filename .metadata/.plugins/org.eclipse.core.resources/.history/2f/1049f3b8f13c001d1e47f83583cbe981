package com.orthofx;

import java.util.*;

public class RepeatingSequence {

	static String substringEval(String str,int start) {
		int i=start,a=0,f=0;
		String x="";
//		int k = Integer.valueOf(Integer.parseInt(""+str.charAt(i-1)));
		while(str.charAt(i)!=']') {
			if(str.charAt(i)=='[') {
				a = i;
			}
			i = i+1;
		}
		int k = Integer.valueOf(Integer.parseInt(""+str.charAt(a-1)));
		String st = str.substring(a+1,i);
		for(int j=0;j<k;++j) {
			x += st;
		}
		for(int j=0;j<str.length();++i) {
			if(str.charAt(i)=='[') {
				f = i;
			}
		}
		String val = str.substring(f+1,a-1) + x;
		return val;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str, sub = "", x="",exp="";
		int k=1,start=0,end = 0,count=0,m=1,st=0,e=0;

		Scanner s = new Scanner(System.in);
		System.out.println("Enter String: ");
		str = s.nextLine();
		for (int i = 0; i < str.length(); ++i) {
			if (str.charAt(i) == '[' && count==0) {
				start = i;
				count = count+1;
			}
		}
		end = str.length()-1;
		k = Integer.valueOf(Integer.parseInt(str.substring(0,start)));
		sub = str.substring(start+1, end);
		exp = substringEval(str,start);
		for(int i=0;i<k;++i) {
			x += exp;
		}
		System.out.println(exp);
	}
}
